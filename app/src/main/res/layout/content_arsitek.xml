<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:showIn="@layout/activity_list_arsitek">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


    <LinearLayout
        android:background="@color/colorPrimary"
        android:orientation="horizontal"
        android:layout_marginTop="0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp">

                <RelativeLayout
                    android:id="@+id/menu1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/foodImage"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/arsitek4" />


                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/propertyName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="23dp"
                        android:layout_marginStart="23dp"
                        android:layout_toEndOf="@+id/foodImage"
                        android:layout_toRightOf="@+id/foodImage"
                        android:layout_weight="1"
                        android:text="Usama Ula"
                        android:textColor="#000"
                        android:textSize="20dp" />

                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/street3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/propertyName"
                        android:layout_alignStart="@+id/propertyName"
                        android:layout_below="@+id/propertyName"
                        android:text="Intresting"
                        android:textColor="#A2A2A2"
                        android:textSize="14dp" />


                </RelativeLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp">

                <RelativeLayout
                    android:id="@+id/menu5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/foodImage2"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/arsitek2" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp" />

                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/propertyName2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="23dp"
                        android:layout_marginStart="23dp"
                        android:layout_toEndOf="@+id/foodImage2"
                        android:layout_toRightOf="@+id/foodImage2"
                        android:layout_weight="1"
                        android:text="Harimukit"
                        android:textColor="#000"
                        android:textSize="20dp" />

                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/street4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/propertyName2"
                        android:layout_alignStart="@+id/propertyName2"
                        android:layout_below="@+id/propertyName2"
                        android:text="Intresting"
                        android:textColor="#A2A2A2"
                        android:textSize="14dp" />
                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp">

                <RelativeLayout
                    android:id="@+id/menu6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/foodImage5"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/arsitek3" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:id="@+id/linearLayout2"
                        tools:ignore="Orientation" />

                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/propertyName4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/linearLayout2"
                        android:layout_marginLeft="28dp"
                        android:layout_marginStart="28dp"
                        android:layout_toEndOf="@+id/foodImage5"
                        android:layout_toRightOf="@+id/foodImage5"
                        android:layout_weight="1"
                        android:text="Gagi Baru"
                        android:textColor="#000"
                        android:textSize="20dp" />

                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/street5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/propertyName4"
                        android:layout_alignStart="@+id/propertyName4"
                        android:layout_below="@+id/propertyName4"
                        android:text="intresting"
                        android:textColor="#A2A2A2"
                        android:textSize="14dp" />





                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp">

                <RelativeLayout
                    android:id="@+id/menu3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/foodImage3"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/arsitek2" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp" />

                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/propertyName6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="25dp"
                        android:layout_marginStart="25dp"
                        android:layout_toEndOf="@+id/foodImage3"
                        android:layout_toRightOf="@+id/foodImage3"
                        android:layout_weight="1"
                        android:text="Halimun"
                        android:textColor="#000"
                        android:textSize="20dp" />

                    <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                        android:id="@+id/street6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/propertyName6"
                        android:layout_alignStart="@+id/propertyName6"
                        android:layout_below="@+id/propertyName6"
                        android:text="Intresting"
                        android:textColor="#A2A2A2"
                        android:textSize="14dp" />

                </RelativeLayout>
            </android.support.v7.widget.CardView>


        </LinearLayout>

    </LinearLayout>

</LinearLayout>
</LinearLayout>